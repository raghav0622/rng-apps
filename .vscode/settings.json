{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".next": true,
    "node_modules": true,
    ".gitignore": true,
    "package-lock.json": true,
    ".prettierrc": true,
    "next-env.d.ts": true,
    ".vscode": true,
    "public": true,
    "eslint.config.mjs": true,
    "next.config.ts": true,
    "tsconfig.json": true,
    ".env.local": true,
    "rng-form": true,
    "lib": true,
    "core/safe-action": true
  },
  "hide-files.files": [
    "node_modules",
    ".gitignore",
    "package-lock.json",
    ".prettierrc",
    "next-env.d.ts",
    ".vscode",
    "public",
    "eslint.config.mjs",
    "next.config.ts",
    "tsconfig.json",
    ".env.local",
    "rng-form",
    "lib",
    "core/safe-action"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
